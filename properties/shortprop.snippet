<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Short Property</Title>
			<Shortcut>shortprop</Shortcut>
			<Description>Code snippet for creating a short property</Description>
			<Author>Christopher Whitley</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
                <Literal>
                    <ID>property</ID>
                    <ToolTip>The name of the property.</ToolTip>
                    <Default>MyShort</Default>
                    <Function></Function>
                </Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[public short $property$ {get; set;}$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
